{
  "--body--background-color": "255, 255, 255",
  "--body--color": "0, 0, 0",
  "--body--font-family": "system-ui, -apple-system, BlinkMacSystemFont, \"Segoe UI\", \"Helvetica Neue\", sans-serif",
  "--body--font-size": "1rem",
  "--body--font-weight": "400",
  "--body--line-height": "1.5",
  "--border--border-color": "128, 128, 128",
  "--border--border-width": "0.125rem",
  "--button--background-color": "128, 128, 128",
  "--button--border-color": "var(--button--background-color)",
  "--button--border-width": "var(--border--border-width)",
  "--button--color": "255, 255, 255",
  "--button--focus--background-color": "0, 0, 0",
  "--button--focus--border-color": "var(--button--focus--background-color)",
  "--button--focus--color": "var(--button--color)",
  "--h1--font-size": "2.75rem",
  "--h2--font-size": "2.25rem",
  "--h3--font-size": "1.75rem",
  "--h4--font-size": "1.5rem",
  "--h5--font-size": "1.25rem",
  "--h6--font-size": "1rem",
  "--heading--font-family": "var(--body--font-family)",
  "--heading--font-weight": "500",
  "--heading--line-height": "1.25",
  "--input--background-color": "255, 255, 255",
  "--input--border-color": "128, 128, 128",
  "--input--border-width": "var(--border--border-width)",
  "--input--checked--background-color": "var(--input--focus--border-color)",
  "--input--checked--border-color": "var(--input--focus--border-color)",
  "--input--color": "var(--body--color)",
  "--input--focus--background-color": "var(--input--background-color)",
  "--input--focus--border-color": "0, 0, 0",
  "--input--focus--color": "var(--input--color)",
  "--label--color": "var(--body--color)",
  "--select--background-color": "var(--input--background-color)",
  "--select--border-color": "var(--input--border-color)",
  "--select--border-width": "var(--input--border-width)",
  "--select--color": "var(--input--color)",
  "--select--focus--background-color": "var(--input--focus--background-color)",
  "--select--focus--border-color": "var(--input--focus--border-color)",
  "--select--focus--color": "var(--input--focus--color)"
}
