  "columns": 12,
  "spacing": [
    {
      "name": "0",
      "size": "0"
    },
    {
      "name": "1",
      "size": "calc(var(--space) * 0.25)"
    },
    {
      "name": "2",
      "size": "calc(var(--space) * 0.5)"
    },
    {
      "name": "3",
      "size": "calc(var(--space) * 0.75)"
    },
    {
      "name": "4",
      "size": "calc(var(--space) * 1)"
    },
    {
      "name": "5",
      "size": "calc(var(--space) * 1.25)"
    },
    {
      "name": "6",
      "size": "calc(var(--space) * 1.5)"
    },
    {
      "name": "7",
      "size": "calc(var(--space) * 1.75)"
    },
    {
      "name": "8",
      "size": "calc(var(--space) * 2)"
    },
    {
      "name": "16",
      "size": "calc(var(--space) * 4)"
    }
  ]
